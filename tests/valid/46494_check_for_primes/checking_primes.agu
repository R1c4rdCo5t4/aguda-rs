-- Considerando um intervalo de numeros de 0 a 20
-- vamos descobrir nrs primos

let _ : Unit =
    let n : Int = 21;
    let prime : Bool[] = new Bool [n | true];

    set prime[0] = false;
    set prime[1] = false;

    let i : Int = 2;
    while i < n do(
        let j : Int = i * 2;
        while j < n do (
            set prime[j] = false;
            set j = j + i
        );
        set i = i + 1
    );

    let k : Int = 0;
    while k < n do (
        if prime[k] then (
            print(k)
        );
        set k = k + 1
    )