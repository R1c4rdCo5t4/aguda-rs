-- Function to sum all numbers from 1 to n
let sum_up_to_n (n) : Int -> Int =
  let sum : Int = 0;
  let i : Int = 1;
  while i <= n do (
    set sum = sum + i;
    set i = i + 1
  );
  sum

let result : Int = sum_up_to_n(5)

let _ : Unit = 
    print(result)  -- Expected result: 15 