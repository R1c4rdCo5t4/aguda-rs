let _ : Unit =
    let i : Int = 0;
    let arr : Int[] = new Int [ 10 | 0 ];
    while i < length(arr) do(
        set arr[i] = i;
        set i = i + 1
    );
    let count : Int = 0;
    let j : Int = 0;
    while j < length(arr) do(
        if arr[j] % 2 == 0 then set count = count + 1;
        set j = j + 1
    );
    print(count)
