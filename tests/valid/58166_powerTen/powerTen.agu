-- Calculates only for positive numbers or 0 and returns -1 for negative values
let powerTen (n) : Int -> Int =
    let result : Int = 0;
    if n < 0 then set result = -1;
    if n == 0 then set result = 1 else set result = 10 * powerTen(n - 1)

let _ : Unit =
    print(powerTen(7))