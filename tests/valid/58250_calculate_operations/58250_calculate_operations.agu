-- Function to perform simple operations
let calculate_operations (a, b, c, d, e) : (Int, Int, Int, Int, Int) -> Int =
  let mult1 : Int = a * b;
  let mult2 : Int = c * d;
  let sub1 : Int = mult1 - mult2;
  let result : Int = sub1 + e;
  result

let result : Int = calculate_operations(2, 3, 4, 5, 6)

let _ : Unit = 
    print(result)  -- Expected result: -8 (6 - 20 + 6)

let x : Int = 1
let y : Int = 2
let z : Int = 3
let w : Int = 4
let v : Int = 5

let final_result : Int = calculate_operations(x, y, z, w, v)

let _ : Unit = 
    print(final_result)  -- Expected result: -5 (2 - 12 + 5)