let stringLength (s) : String -> String =
    let text : String =
        if length(s) == 0 then "Empty String" else
        if length(s) > 0 && length(s) < 5 then "Tiny String" else
        if length(s) >= 5 && length(s) < 10 then "Big String" else
        "WHOAH... Calm Down";
    text


let _ : Unit =
    let s1 : String = "Ornitorrinco";
    let s2 : String = "Manel";
    let s3 : String = "Casa";
    print(stringLength(s1));
    print("\n");
    print(stringLength(s2));
    print("\n");
    print(stringLength(s3))
