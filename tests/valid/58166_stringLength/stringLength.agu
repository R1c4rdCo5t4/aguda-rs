let stringLength (s) : String -> String =
    let text : String = "";
    if length(s) == 0 then set text = "Empty String" else
    if length(s) > 0 && length(s) < 5 then set text = "Tiny String" else
    if length(s) >= 5 && length(s) < 10 then set text = "Big String" else
    if length(s) >= 10 then set text = "WHOAH... Calm Down";
    text

let _ : Unit =
    let s1 : String = "Ornitorrinco";
    let s2 : String = "Manel";
    let s3 : String = "Casa";
