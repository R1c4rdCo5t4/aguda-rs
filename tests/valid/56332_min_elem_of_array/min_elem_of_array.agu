let minArray (a) : Int[] -> Int =
    let i : Int = 1;
    let min : Int = a[0];
    while i < length(a) do (
        if a[i] < min then set min = a[i];
        set i = i + 1
    );
    min

let _ : Unit = print(minArray(new Int[] [6 | 0 ; -1 ; 10 ; 0 ; -50 ; 100]))
