let printMatrix (a) : Int[][] -> Unit =
    let i : Int = 0;
    while i < length(a) do (
        let j : Int = 0;
        while j < length(a[0]) do (
            print(a[i][j]); print(" ");
            set j = j + 1
        );
        print("\n");
        set i = i + 1
    )

let main : Unit =
    let a : Int[][] = new Int[][2 | new Int[2 | 0]];
    set a[0][0] = 1;
    set a[1][1] = 1;
    printMatrix(a)